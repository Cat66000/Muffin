{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false,
        "better-docs": {
            "name": "MuffinDB Documentation"
        }
    },
    "opts": {
        "template": "./node_modules/better-docs",
        "readme": "./README_JSDoc.md"
    }
}